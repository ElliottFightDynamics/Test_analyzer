# MongoDB or PostgreSQL configurations
DB_HOST=localhost
DB_PORT=27017
DB_NAME=codebase_analysis

# Node.js server configurations
SERVER_PORT=3000

# AI/ML service configurations
AI_SERVICE_HOST=localhost
AI_SERVICE_PORT=5000

# Security configurations
JWT_SECRET=your_jwt_secret
ENCRYPTION_SALT_ROUNDS=10

# External API configurations (if any)
GITHUB_API_TOKEN=your_github_api_token
BITBUCKET_API_KEY=your_bitbucket_api_key

# Other configurations
LOG_LEVEL=info
MAX_UPLOAD_SIZE=50MB # Adjust as needed
MAX_CONCURRENT_UPLOADS=5 # Adjust as needed

# Environment (development, production, etc.)
NODE_ENV=development